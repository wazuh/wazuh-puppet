  <integration>
    <name><%= @ossec_integration_name %></name>
    <% if @ossec_integration_hook_url != '' -%>
    <hook_url><%= @ossec_integration_hook_url %></hook_url>
    <% end %>
    <% if @ossec_integration_api_key != '' -%>
    <api_key><%= @ossec_integration_api_key %></api_key>
    <% end %>
    <% if @ossec_integration_rule_id != '' -%>
    <rule_id><%= @ossec_integration_rule_id %></rule_id>
    <% end %>
    <% if @ossec_integration_level != '' -%>
    <level><%= @ossec_integration_level %></level>
    <% end %>
    <% if @ossec_integration_group != '' -%>
    <group><%= @ossec_integration_group %></group>
    <% end %>
    <% if @ossec_integration_event_location != '' -%>
    <event_location><%= @ossec_integration_event_location %></event_location>
    <% end %>
    <% if @ossec_integration_alert_format != '' -%>
    <alert_format><%= @ossec_integration_alert_format %></alert_format>
    <% end %>
    <% if @ossec_integration_max_log != '' -%>
    <max_log><%= @ossec_integration_max_log %></max_log>
    <% end %>
  </integration>


