  <localfile>
    <log_format><%= @logtype %></log_format>
    <%- if @logfile -%>
    <location><%= @logfile %></location>
    <%- end -%>
    <%- if @logcommand -%>
    <command><%= @logcommand %> </command>
    <%- end -%>
    <%- if @commandalias -%>
    <alias><%= @commandalias %> </alias>
    <%- end -%>
